{
  "compilerOptions": {
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/",
    "noImplicitAny": true,
    "module": "esNext",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"]
    },
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
